{
    "title": "Origin Information Schema",
    "$id": "https://static.echo-chat.au/schemas/meta/origin.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": [
        "host", "port"
    ],
    "properties": {
        "host": {
            "type": "string",
            "format": "url"
        },
        "port": {
            "type": "integer"
        }
    }
}